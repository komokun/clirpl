{
  "name": "clirpl",
  "author": "Khosi Morafo",
  "version": "0.0.1",
  "description": "",
  "main": "clirpl.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "serve": "NODE_ENV=production node dist/clirpl.js",
    "start": "NODE_ENV=development babel-node src/clirpl.js",
    "dev": "DEBUG=server:debug NODE_ENV=development nodemon src/clirpl.js --exec babel-node",
    "test": "DEBUG=server:debug NODE_ENV=test mocha --require babel-core/register --reporter spec --exit   tests/ --exec babel-node",
    "test:watch": "DEBUG=server:debug NODE_ENV=development mocha  --require babel-core/register --watch --reporter  spec   tests/ --exec babel-node",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "chai": "^4.2.0",
    "config": "^3.1.0",
    "debug": "^4.1.1",
    "mocha": "^5.2.0",
    "supertest": "^3.4.2"
  },
  "babel": {
    "presets": [
      "node8"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": 0,
      "node/no-unsupported-features/es-builtins": 0
    },
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "chalk": "^2.4.2",
    "cli-table3": "^0.5.1",
    "clui": "^0.3.6",
    "colors": "^1.3.3",
    "console2": "^2.3.7",
    "fs-extra": "^8.1.0",
    "js-function-string": "^1.0.0",
    "jspm": "^2.0.0-beta.6",
    "lodash": "^4.17.13",
    "log-symbols": "^3.0.0",
    "minimist": "^1.2.0",
    "nodemon": "^1.19.1",
    "ora": "^3.4.0",
    "osascript": "^1.2.0",
    "require-dir": "^1.2.0",
    "ripple-lib": "^1.2.4",
    "rippled-ws-client": "^1.0.0",
    "shelljs": "^0.8.3",
    "table": "^5.4.1",
    "through2": "^3.0.1",
    "vorpal": "^1.4.0"
  }
}
